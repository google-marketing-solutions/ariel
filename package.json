{
	"name": "ariel",
	"version": "1.0.0",
	"description": "Ariel - AI-powered video dubbing",
	"keywords": [
		"google-cloud",
		"video",
		"ai",
		"dubbing"
	],
	"scripts": {
		"build": "tsc",
		"start": "npm i --silent && npm run build && node dist/index.js",
		"deploy-ui": "cd ui && npm i --silent && npm run deploy",
		"deploy-backend": "cd backend && bash deploy.sh"
	},
	"engines": {
		"node": ">=12"
	},
	"dependencies": {
		"cross-spawn": "^7.0.3",
		"fs-extra": "^11.2.0",
		"prompts": "^2.4.2",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/cross-spawn": "^6.0.6",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.12.7",
		"@types/prompts": "^2.4.9",
		"replace": "^1.2.2"
	}
}
